        <activity
            android:name="com.example.admin.DashboardActivity"
            android:theme="@style/AppTheme.NoActionBar_TransparentStatusBar" />
        <activity android:name="com.example.admin.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>