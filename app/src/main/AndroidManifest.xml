        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.admin.activity.BusStatistics"
            android:exported="true" />
        <activity
            android:name="com.example.admin.activity.AddPlane"
            android:exported="true" />
        <activity
            android:name="com.example.admin.activity.Forget_Activity"
            android:exported="true" />
        <activity
            android:name="com.example.admin.activity.AddTrain"
            android:exported="true" />
        <activity
            android:name="com.example.admin.activity.DataLogActivity"
